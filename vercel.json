{
  "functions": {
    "api/chat.ts": {
      "maxDuration": 15
    },
    "api/secure-store.js": {
      "maxDuration": 5
    },
    "api/og-image.js": {
      "maxDuration": 5
    },
    "api/redirect.js": {
      "maxDuration": 3
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat"
    },
    {
      "src": "/api/secure-store",
      "dest": "/api/secure-store"
    },
    {
      "src": "/api/og-image",
      "dest": "/api/og-image"
    },
    {
      "src": "/api/redirect",
      "dest": "/api/redirect"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}