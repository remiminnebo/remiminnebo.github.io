{
  "functions": {
    "api/chat.ts": {
      "maxDuration": 30
    },
    "api/share.js": {
      "maxDuration": 10
    },
    "api/og-image.js": {
      "maxDuration": 10
    },
    "api/redirect.js": {
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}